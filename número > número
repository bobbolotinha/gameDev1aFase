programa {
  funcao inicio() {
    inteiro numeroUm , numeroDois 

    escreva("digite um número: ")
    leia(numeroUm)

    escreva("digite outro numero: ")
    leia(numeroDois)

    se(numeroUm > numeroDois){

    escreva("esse número: ", numeroUm ," é maior que este número: ", numeroDois)
    }senao se( numeroDois > numeroUm){

      escreva(" esse número: ", numeroDois, " é maior que este número: ", numeroUm)
    }senao se(numeroUm == numeroDois){

     escreva("esses números tem os mesmos valores")

    }
 
 
 
  }
}
